{
  "name": "@nowprototypeit/docs",
  "version": "0.0.1",
  "description": "Documentation for Now Prototype It",
  "type": "module",
  "scripts": {
    "dev": "watch-and-test --watch-file-extensions='ts,json,md' 'npm run check' 'npm run lint:fix' 'npm run build' 'npm run dev-server'",
    "dev-server": "PORT=3013 nodemon server.ts",
    "build": "node build.ts",
    "check": "tsc --noEmit --pretty",
    "lint": "eslint . --ext .ts",
    "lint:fix": "npm run lint -- --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@nowprototypeit/design-system": "^1.0.1",
    "express": "^4.18.3",
    "marked": "^12.0.1",
    "nunjucks": "^3.2.4",
    "sass": "^1.71.1",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^24.0.10",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript-eslint": "^8.35.1",
    "watch-and-test": "^0.0.2"
  }
}
